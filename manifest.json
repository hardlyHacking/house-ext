{
  "manifest_version": 2,

  "name": "PercentRules",
  "description": "Find and analyze housing investments quickly on popular sites like Zillow.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "PercentRules"
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}